.custom-tabs{width:100%}.tab-headers{display:flex;justify-content:space-around;background-color:#fff;padding:10px 30px}.tab-header{width:45%;cursor:pointer;padding:8px 20px;border-bottom:2px solid transparent;border-radius:15px;box-shadow:4px 4px 20px rgba(110,170,172,.2);display:flex;flex-direction:row;justify-content:center;-webkit-tap-highlight-color:transparent}.tab-icon{margin-right:10px}.tab-title{font-size:25rpx;color:#6eaaac;text-align:center}.tab-header.active{border-color:#6eaaac;transition:all .3s ease;transform:scale(1.05);box-shadow:4px 4px 20px rgba(110,170,172,.2)}.tab-content{background-color:#fff;overflow-y:auto;height:40vh}
